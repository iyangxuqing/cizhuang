.container {
  padding-left: 30rpx;
  padding-right: 30rpx;
}

.head {
  margin-left: -30rpx;
  margin-right: -30rpx;
  margin-bottom: 30rpx;
}

.head-images {
  height: 440rpx;
}

.head-title {
  height: 100rpx;
  line-height: 100rpx;
  font-size: 36rpx;
  text-align: center;
  color: #333;
}

.head-logo {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100rpx;
  padding: 0 180rpx;
  /* background: #07c56f; */
  background-color: #d5d4d4;
}

.list{
  display: flex;
  flex-wrap: wrap;
  font-size: 28rpx;
  justify-content: space-between;
}

.list .item {
  display: flex;
  flex-direction: column;
  width: 330rpx;
  margin-bottom: 40rpx;
}

.list .item .thumb {
  width: 330rpx;
  height: 300rpx;
  border-radius: 8rpx;
  overflow: hidden;
  border: 1px solid #eee;
}

.list .item .title {
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 28rpx;
}